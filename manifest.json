{
    "background": {
        "service_worker": "bg-wrapper.js"
    },
    "action": {
        "browser_style": true,
        "default_icon": {
            "16": "logo2.png",
            "32": "logo2.png"
        },
        "default_title": "AnimeHunter",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "css": [
                "css/style.css"
            ],
            "js": [
                "js/jquery-3.5.1.min.js",
                "js/inject.js"
            ],
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_start"
        }
    ],
    "description": "Уведомляет вас о свежих сериях аниме вышедших на популярных фандаб сайтах",
    "icons": {
        "128": "logo2.png",
        "16": "logo2.png",
        "48": "logo2.png"
    },
    "manifest_version": 3,
    "name": "Anime Hunter",
    "permissions": [
        "storage",
        "unlimitedStorage",
        "notifications",
        "tabs"
    ],
    "short_name": "Anime Hunter",
    "update_url": "https://extension-updates.opera.com/api/omaha/update/",
    "version": "0.1.5.8"
}
